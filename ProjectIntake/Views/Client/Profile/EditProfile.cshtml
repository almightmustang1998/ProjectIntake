@{
    Layout = null; // No layout to match the wireframe
}
<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="~/css/ProjectIntake.css">
</head>
<body>
    <div id="header">
        <nav>
            <ul>
                <li><a asp-controller="ClientDashboard" asp-action="ClientDashboard">Project Intake</a></li>
                <li><a asp-controller="ClientDashboard" asp-action="ClientDashboard">Dashboard</a></li>
                <li><a asp-controller="ClientViewProjects" asp-action="ClientViewProjects">View Submitted Projects</a></li>
                <li><a asp-controller="EditProfile" asp-action="EditProfile">Edit Profile</a></li>
                <li><a asp-controller="Account" asp-action="AccountLogin">Logout</a></li>
            </ul>
        </nav>
    </div>

    <div id="content">
        <div id="profile-edit-form">
            <h2>Edit Profile</h2>
                <!-- Update the action attribute to your form handler endpoint -->
                <label for="firstName">First Name:</label>
                <input type="text" id="firstName" name="firstName" value="Nathan">

                <label for="lastName">Last Name:</label>
                <input type="text" id="lastName" name="lastName" value="Marzina">

                <label for="email">Email:</label>
                <input type="email" id="email" name="email" value="NJMarzina@gmail.com">

                <label for="organization">Organization:</label>
                <input type="text" id="organization" name="organization" value="Temple University">

                <input type="submit" value="Submit Changes" class="button-link">
        </div>
    </div>
</body>
</html>
